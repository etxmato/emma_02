<?xml version="1.0"?>
<!DOCTYPE emmaconfig SYSTEM "emma02.dtd">

<emmaconfig>
    
    <system>
        <name>COSMAC Elf</name>
        <config>Led &amp; Keypad</config>
        <clock>1.76</clock>
        <init type="ef1">1</init>
        <init type="ef2">1</init>
        <init type="ef3">1</init>
        <init type="ef4">1</init>
        <dirname>Elf</dirname>
    </system>

    <frontpanel type="cosmac" init="on">
        <out>4</out>
        <in>4</in>
        <ef type="in">4</ef>
        <til>313</til>
        <led />
    </frontpanel>

    <keypad type="elf">
        <in>4</in>
        <ef type="ready">3</ef>
    </keypad>

    <memory>
         <guiram>
            <start>0x0000</start>
            <end>0xFFFF</end>
        </guiram>
    </memory>
    
    <debugger>
        <scrt mode="off">
            <call reg="4">-1</call>
            <return reg="5">-1</return>
        </scrt>

        <assembler>
            <filename>mycode</filename>
            <code_start>0</code_start>
            <code_end>0xFFF</code_end>
        </assembler>
    </debugger>

</emmaconfig>
