<?xml version="1.0"?>
<!DOCTYPE emmaconfig SYSTEM "emma02.dtd">

<emmaconfig>
    
    <system>
        <name>Bare Microtutor</name>
        <clock>1.75</clock>
        <init type="ef1">1</init>
        <init type="ef2">1</init>
        <init type="ef3">1</init>
        <init type="ef4">1</init>
        <dirname>Microtutor</dirname>
    </system>
    
    <frontpanel type="microtutor" init="on">
        <out>0</out>
        <in>0</in>
        <ef type="in">4</ef>
    </frontpanel>

     <memory>
        <mainram>
            <start>0x0000</start>
            <end>0x00FF</end>
        </mainram>
    </memory>
    
</emmaconfig>
