        </item>
        <item>
            <type>switch</type>
            <function>mp</function>
            <init>up</init>
            <pos>127, 210</pos>
        </item>
        <item>
            <type>switch</type>
            <value>7</value>
            <function>bitswitch</function>
            <pos>194, 210</pos>
        </item>
        <item>
            <type>switch</type>
            <value>6</value>
            <function>bitswitch</function>
            <pos>228, 210</pos>
        </item>
        <item>
            <type>switch</type>
