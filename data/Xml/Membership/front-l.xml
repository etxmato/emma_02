            <function>multitil</function>
            <pos>317, 23</pos>
        </item>
        <item>
            <type font="italic">til</type>
            <value address="10">3</value>
            <function>multitil</function>
            <pos>345, 23</pos>
        </item>
        <item>
            <type font="italic">til</type>
            <value>4</value>
            <function>multitil</function>
            <pos>373, 23</pos>
        </item>
        <item>
            <type font="italic">til</type>
            <value>5</value>
            <function>multitil</function>
            <pos>401, 23</pos>
        </item>
        <item>
            <type form="largegreen">led</type>
            <function>qled</function>
            <pos>25, 136</pos>
        </item>
        <item>
            <type form="large">led</type>
            <value>7</value>
            <function load="show" cycle="show">bitled</function>
            <pos>194, 136</pos>
        </item>
        <item>
            <type form="large">led</type>
            <value>6</value>
            <function load="show" cycle="show">bitled</function>
            <pos>228, 136</pos>
        </item>
        <item>
            <type form="large">led</type>
