    <memory>
        <nvram type="gui">
            <start>0x0</start>
            <end>0xFFFF</end>
            <dump>ramdump.bin</dump>
            <mp>switch</mp>
            <pulldown mask="*.hex">Membership</pulldown>
            <filename type="multitil">banner.hex</filename>
        </nvram>
    </memory>
    
    <debugger>
        <scrt mode="off">
            <call reg="4">-1</call>
            <return reg="5">-1</return>
        </scrt>
    </debugger>
    
</emmaconfig>
